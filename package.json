{
  "name": "protobuf-extractor",
  "version": "0.0.0-development",
  "license": "MIT",
  "author": "Jonluca DeCaro <protobufextractor@jonlu.ca>",
  "description": "Extracts protobuf definitions from a javascript file",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "type": "module",
  "keywords": [
    "protobuf",
    "proto",
    "extractor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonluca/protobuf-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/jonluca/protobuf-extractor/issues"
  },
  "homepage": "https://github.com/jonluca/protobuf-extractor",
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "eslint src",
    "build": "rm -fr dist/* && tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.+(js|jsx|ts|tsx|har||json|css|md)\"",
    "test": "vitest --coverage",
    "test:update": "vitest -u",
    "test:watch": "vitest -w"
  },
  "dependencies": {
    "@types/esprima": "^4.0.3",
    "@types/estraverse": "^5.1.2",
    "@types/jsdom": "^20.0.0",
    "@types/tough-cookie": "^4.0.2",
    "axios": "^1.1.3",
    "esprima": "^4.0.1",
    "estraverse": "^5.3.0",
    "http-cookie-agent": "^4.0.2",
    "jsdom": "^20.0.2",
    "lodash-es": "^4.17.21",
    "tough-cookie": "^4.1.2"
  },
  "devDependencies": {
    "@types/eslint": "8.4.9",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@types/sort-json": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitest/coverage-c8": "^0.24.4",
    "c8": "^7.12.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "4.8.4",
    "vitest": "^0.24.4"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix"
    ],
    "**/*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
